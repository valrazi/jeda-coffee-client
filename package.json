{
  "name": "live-ticket-report",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4003 --turbopack",
    "build": "next build",
    "start": "next start -p 4003",
    "lint": "next lint",
    "migrate:create": "npx sequelize-cli migration:generate --name",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:one": "npx sequelize-cli db:migrate --name",
    "migrate:down": "npx sequelize-cli db:migrate:undo",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && sequelize db:migrate",
    "migrate:undo:one": "npx sequelize-cli db:migrate:undo --name",
    "seed:create": "npx sequelize-cli seed:generate --name",
    "seed:up": "npx sequelize-cli db:seed:all",
    "seed:one": "npx sequelize-cli db:seed --seed",
    "seed:down": "npx sequelize-cli db:seed:undo --name",
    "seed:reset": "npx sequelize-cli db:seed:undo:all"
  },
  "dependencies": {
    "@ant-design/icons": "^5.6.1",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "antd": "^5.24.4",
    "axios": "^1.8.3",
    "bcryptjs": "^3.0.2",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "leaflet": "^1.9.4",
    "mysql2": "^3.13.0",
    "next": "15.2.3",
    "next-auth": "^4.24.11",
    "pg-hstore": "^2.3.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-leaflet": "^5.0.0",
    "react-quill-new": "^3.4.1",
    "react-webcam": "^7.2.0",
    "sequelize": "^6.37.6",
    "sequelize-cli": "^6.6.2",
    "sweetalert2": "^11.6.13"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "tailwindcss": "^4"
  }
}
